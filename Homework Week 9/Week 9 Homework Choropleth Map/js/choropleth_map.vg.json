{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Employed person by sex and state 1982 2020 Malaysia",
  "width": 800,
  "height": 600,
  "data": {
    "name": "malaysia_map",
    "url": "https://github.com/daniel-gus/FIT3179/blob/fe56baa705bbddd426da0c12728cd09cb22f22e0/Additional%20Data%20Files/state_boundaries.json",
    "format": {"property": "features"}
},
  "transform": [{
      "lookup": "properties.state/country",
      "from": {
        "data": {
          "url": "https://github.com/daniel-gus/FIT3179/blob/fe56baa705bbddd426da0c12728cd09cb22f22e0/Visualisation%202%20Datasets/Employed%20person%20by%20sex%20and%20state%201982%20%20%202020%20Malay.csv"
        },
        "key": "State/Country",
        "fields": ["Active"]
      }
    },
    {"calculate": "datum.Active + 0.1", "as": "Active Cases"}
  ],
  "projection": {
    "type": "equirectangular"
  },
  "mark": {
    "type": "geoshape"
    },
  "encoding": {
    "color": {
      "field": "Active Cases",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country"},
      {"field": "Active", "type": "quantitative"}
    ]
  }


  
}
