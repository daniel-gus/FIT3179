{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Employed Population in Malaysia",
    "width": 800,
    "height": 600,
    "data": {
      "url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation%202/js/state_boundaries_topojson.json",
      "format": {
            "type": "topojson",
            "feature": "state_boundaries"
          }
    },
    "transform": [{
        "lookup": "properties.state",
        "from": {
          "data": {
            "url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation%202/Datasets/cleaned%20Employed%20person%20by%20sex%20and%20state%201982%20to%202020%20Malay.csv"
          },
          "key": "state",
          "fields": ["sum_employed"]
        }
      }
    ],
    "projection": {
      "type": "equirectangular"
    },
    "mark": {
      "type": "geoshape"
      },
    "encoding": {
      "color": {
        "field": "sum_employed",
        "type": "quantitative",
        "scale": {"type": "log"}
      },
      "tooltip": [
        {"field": "properties.state", "type": "nominal", "title": "state"},
        {"field": "sum_employed", "type": "quantitative"}
      ]
    }
  
  }