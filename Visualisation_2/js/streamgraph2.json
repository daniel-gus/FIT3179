{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation_2/Datasets/Employed_persons_by_ethnic_group_and_state_1982_2020.csv"
  },
  "mark": {"type": "circle", "size": 1200},
  "transform": [{"calculate": "datum.EthnicGroup", "as": "Ethnic Group"}],
  "encoding": {
    "x": {"field": "Year", "type": "ordinal", "axis": null},
    "y": {"field": "State", "type": "nominal", "axis": null},
    "color": {
      "field": "EthnicGroup",
      "type": "nominal",
      "scale": {
        "type": "ordinal",
        "domain": ["Bumiputera", "Chinese", "Indians", "Others"],
        "range": ["#53280b", "#8f6448", "#e2bf98", "#ffe599"]
      },
      "legend": {"title": "Ethnic Group"}
    }
  },
  "config": {"style": {"cell": {"stroke": "transparent"}}}
}