{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation_2/Datasets/Employed_persons_by_ethnic_group_and_state_1982_2020.csv"
  },
  "mark": {"type": "circle", "size": 1200},
  "transform": [{"calculate": "datum.vax_type", "as": "Vaccination Type"}],
  "encoding": {
    "x": {"field": "column", "type": "quantitative", "axis": null},
    "y": {"field": "row", "type": "quantitative", "axis": null},
    "color": {
      "field": "vax_type",
      "type": "nominal",
      "scale": {
        "type": "ordinal",
        "domain": ["full", "partial", "unvaccinated"],
        "range": ["#31a354", "#bae4b3", "#ECECEC"]
      },
      "legend": {"title": "Vaccination Type"}
    }
  },
  "config": {"style": {"cell": {"stroke": "transparent"}}}
}