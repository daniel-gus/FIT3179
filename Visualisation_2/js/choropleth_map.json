{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Employed Population in Malaysia",
  "width": 550,
  "height": 400,
  "data": {
    "url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation%202/js/state_boundaries_topojson.json",
    "format": {
          "type": "topojson",
          "feature": "state_boundaries"
        }
  },
  "transform": [{
      "lookup": "properties.state",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation_2/Datasets/Employed_person_by_sex_and_state_1982_to_2020_Malaysia.csv"
        },
        "key": "state",
        "fields": ["sum_employed"]
      }
    }
  ],
  "projection": {
    "type": "equirectangular"
  },
  "mark": {
    "type": "geoshape"
    },
  "encoding": {
    "color": {
      "field": "sum_employed",
      "type": "quantitative",
      "scale": {"type": "log"}
    },
    "tooltip": [
      {"field": "properties.state", "type": "nominal", "title": "state"},
      {"field": "sum_employed", "type": "quantitative"}
    ]
  }

}