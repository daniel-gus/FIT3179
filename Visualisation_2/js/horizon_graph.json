{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "Horizon Graph with 2 layers. (See https://idl.cs.washington.edu/papers/horizon/ for more details on Horizon Graphs.)",
    "width": 300,
    "height": 300,
    "data": {"url": "https://raw.githubusercontent.com/daniel-gus/FIT3179/master/Visualisation_2/Datasets/Employed_persons_by_marital_status_and_state_Malaysia.csv"},
    "transform": [
        {"filter": "datum.Year == 2020"},
        {"filter": "datum.State == 'JOHOR'"}
    ],
    "encoding": {
      "x": {
        "field": "MaritalStatus", "type": "nominal",
        "scale": {"zero": false, "nice": false}
      },
      "y": {
        "field": "Person", "type": "quantitative",
        "scale": {"domain": [0,1000000]},
        "axis": {"title": "Person"}
      }
    },
    "layer": [{
      "mark": {"type": "area", "clip": true, "orient": "vertical", "opacity": 0.6}
    }, {
      "transform": [{"calculate": "datum.Person - 50", "as": "ny"}],
      "mark": {"type": "area", "clip": true, "orient": "vertical"},
      "encoding": {
        "y": {
          "field": "ny", "type": "quantitative",
          "scale": {"domain": [0,1000000]}
        },
        "opacity": {"value": 0.3}
      }
    }],
    "config": {
      "area": {"interpolate": "monotone"}
    }
  }